{
  "env": {
    "es2022": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "no-console": "off",
    "no-trailing-spaces": "error",
    "eol-last": "error",
    "camelcase": "error",
    "no-var": "error",
    "prefer-const": "error"
  }
}
